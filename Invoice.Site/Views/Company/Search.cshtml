@model Invoice.Site.Models.Company.CompanySearchResultViewModel

<div class="panel panel-default panel-table">
    <div class="panel-body company-list-body">
        <table class="table table-striped table-bordered table-list company-list-table">
            <thead>
                <tr>
                    <th>Actions</th>
                    <th>Symbol</th>
                    <th>Nip</th>
                    <th>Name</th>
                    <th>Description</th>
                </tr>
            </thead>
            <tbody>
                @foreach (var company in Model.SearchResultList)
                {
                    @Html.Partial(MVC.Company.Views._CompanyList, company);
                }
            </tbody>
        </table>
    </div>
</div>
